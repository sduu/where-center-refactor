{
  "singleQuote": true,
  "jsxSingleQuote": true,
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 120,
  "arrowParens": "avoid",
  "importOrder": [
    "^react(.*)",
    "<THIRD_PARTY_MODULES>",
    "^@/pages/(.*)$",
    "^@/common/(.*)$",
    "^@/components/(.*)$",
    "^@/styles/(.*)$",
    "@/(.*)",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}
